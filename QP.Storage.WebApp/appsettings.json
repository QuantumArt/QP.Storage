{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ImageResizeSettings": {
    "SearchParameter": "size",
    "WidthParameter": "w",
    "ExtensionsAllowedToResize": [
      ".png",
      ".jpg",
      ".jpeg",
      ".webp",
      ".gif"
    ],
    "ResizedImageTemplate": "{0}-{1}{2}",
    "ReduceSizes": [
      {
        "ReduceRatio": 2,
        "Postfix": "medium"
      },
      {
        "ReduceRatio": 3,
        "Postfix": "small"
      }
    ]
  }
}
